{
  "expo": {
    "name": "Ascent Beacon",
    "slug": "ascent-beacon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/login-background.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ascentbeacon.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ascentbeacon.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "ascentbeacon",
    "plugins": ["expo-web-browser", "expo-secure-store"]
  }
}
